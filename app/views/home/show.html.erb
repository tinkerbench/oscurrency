<%= column_div :type => :primary do -%>
  <%- unless global_prefs.app_name.blank? -%>
  <h2><%= global_prefs.app_name %> <%= t('home.show.header') %></h2>
  <%- else -%>
  <h2><%= t('home.show.header').capitalize %></h2>
  <%- end -%>
<%- end -%>

<%= column_div :type => :secondary do -%>
  <%= render :partial => 'secondary_column' %>
<%- end -%>
